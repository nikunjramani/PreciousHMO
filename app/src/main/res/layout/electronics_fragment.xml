<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".client.activities.fragments.electronics.ElectronicsFrag">

    <androidx.cardview.widget.CardView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ProgressBar
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:id="@+id/progressBar"

                />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Enrolee ID"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:id="@+id/tvIdTitle"
                android:layout_marginTop="100dp"
                android:layout_marginStart="20dp"
                android:textColor="@android:color/black"
                />

            <TextView
                android:id="@+id/tvId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:textColor="@android:color/black"
                app:layout_constraintStart_toEndOf="@id/tvAddressTitle"
                app:layout_constraintTop_toTopOf="@id/tvIdTitle" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name"
                app:layout_constraintStart_toStartOf="@id/tvIdTitle"
                app:layout_constraintTop_toBottomOf="@id/tvIdTitle"
                android:id="@+id/tvNameTitle"
                android:layout_marginTop="8dp"
                android:textColor="@android:color/black"
                />

            <TextView
                android:id="@+id/tvName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                app:layout_constraintStart_toStartOf="@id/tvId"
                app:layout_constraintTop_toTopOf="@id/tvNameTitle" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/email"
                app:layout_constraintStart_toStartOf="@id/tvNameTitle"
                app:layout_constraintTop_toBottomOf="@id/tvNameTitle"
                android:id="@+id/tvEmailTitle"
                android:layout_marginTop="8dp"
                android:textColor="@android:color/black"
                />

            <TextView
                android:id="@+id/tvEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                app:layout_constraintStart_toStartOf="@id/tvName"
                app:layout_constraintTop_toTopOf="@id/tvEmailTitle" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/contact_no"
                app:layout_constraintStart_toStartOf="@id/tvEmailTitle"
                app:layout_constraintTop_toBottomOf="@id/tvEmailTitle"
                android:id="@+id/tvContactTitle"
                android:layout_marginTop="8dp"
                android:textColor="@android:color/black"
                />

            <TextView
                android:id="@+id/tvContact"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                app:layout_constraintStart_toStartOf="@id/tvEmail"
                app:layout_constraintTop_toTopOf="@id/tvContactTitle" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/sex"
                app:layout_constraintStart_toStartOf="@id/tvContactTitle"
                app:layout_constraintTop_toBottomOf="@id/tvContactTitle"
                android:id="@+id/tvSexTitle"
                android:layout_marginTop="8dp"
                android:textColor="@android:color/black"
                />

            <TextView
                android:id="@+id/tvSex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                app:layout_constraintStart_toStartOf="@id/tvContact"
                app:layout_constraintTop_toTopOf="@id/tvSexTitle" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/marital_status"
                app:layout_constraintStart_toStartOf="@id/tvSexTitle"
                app:layout_constraintTop_toBottomOf="@id/tvSexTitle"
                android:id="@+id/tvMaritalStatusTitle"
                android:layout_marginTop="8dp"
                android:textColor="@android:color/black"
                />

            <TextView
                android:id="@+id/tvMaritalStatus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                app:layout_constraintStart_toStartOf="@id/tvSex"
                app:layout_constraintTop_toTopOf="@id/tvMaritalStatusTitle" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/date_of_birth"
                app:layout_constraintStart_toStartOf="@id/tvMaritalStatusTitle"
                app:layout_constraintTop_toBottomOf="@id/tvMaritalStatusTitle"
                android:id="@+id/tvDateTitle"
                android:layout_marginTop="8dp"
                android:textColor="@android:color/black"
                />

            <TextView
                android:id="@+id/tvDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                app:layout_constraintStart_toStartOf="@id/tvMaritalStatus"
                app:layout_constraintTop_toTopOf="@id/tvDateTitle" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/state"
                app:layout_constraintStart_toStartOf="@id/tvDateTitle"
                app:layout_constraintTop_toBottomOf="@id/tvDateTitle"
                android:id="@+id/tvStateTitle"
                android:layout_marginTop="8dp"
                android:textColor="@android:color/black"
                />

            <TextView
                android:id="@+id/tvState"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                app:layout_constraintStart_toStartOf="@id/tvDate"
                app:layout_constraintTop_toTopOf="@id/tvStateTitle" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/resident_address"
                app:layout_constraintStart_toStartOf="@id/tvStateTitle"
                app:layout_constraintTop_toBottomOf="@id/tvStateTitle"
                android:id="@+id/tvAddressTitle"
                android:layout_marginTop="8dp"
                android:textColor="@android:color/black"
                />

            <TextView
                android:id="@+id/tvAddress"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/tvState"
                app:layout_constraintTop_toTopOf="@id/tvAddressTitle" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/current_provider"
                app:layout_constraintStart_toStartOf="@id/tvAddressTitle"
                app:layout_constraintTop_toBottomOf="@id/tvAddress"
                android:id="@+id/tvProviderTitle"
                android:layout_marginTop="8dp"
                android:textColor="@android:color/black"
                />

            <TextView
                android:id="@+id/tvProvider"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/tvAddress"
                app:layout_constraintTop_toTopOf="@id/tvProviderTitle" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/organization"
                app:layout_constraintStart_toStartOf="@id/tvProviderTitle"
                app:layout_constraintTop_toBottomOf="@id/tvProviderTitle"
                android:id="@+id/tvOrganizationTitle"
                android:layout_marginTop="8dp"
                android:textColor="@android:color/black"
                />

            <TextView
                android:id="@+id/tvOrganization"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/tvProvider"
                app:layout_constraintTop_toTopOf="@id/tvOrganizationTitle" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>